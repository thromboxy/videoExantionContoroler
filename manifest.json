{
   "content_scripts": [ {
      "permissions": ["scripting", "storage"],
      "all_frames": false,
      "js": [ "js/common.js", "js/main-youtube.js" ],
      "matches": [ "https://www.youtube.com/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": false,
      "js": [ "js/common.js", "js/main-niconico.js" ],
      "matches": [ "https://www.nicovideo.jp/watch/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": false,
      "js": [ "js/common.js", "js/main-twitch.js" ],
      "matches": [ "https://www.twitch.tv/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": false,
      "js": [ "js/common.js", "js/main-amazon.js" ],
      "matches": [ "https://www.amazon.co.jp/*" ],
      "run_at": "document_end"
   }, {
      "all_frames": false,
      "js": [ "js/common.js", "js/main-danime.js" ],
      "matches": [ "https://anime.dmkt-sp.jp/*" ],
      "run_at": "document_end"
   } ],
   "content_security_policy": {
      "default-src": "self"
   },
   "description": "Chrome Extension for video controller",
   "icons": {
      "128": "icon.png"
   },
   "manifest_version": 3,
   "name": "Video Expansion Controller",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.3.3"
}
